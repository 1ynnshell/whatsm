  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.3",
    "@vitest/eslint-plugin": "1.1.10",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.14.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.3.3",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.6.3",
    "vitest": "^2.1.5"
  }
